{
  "name": "omp-server",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "npm-run-all -p -r -l dev:watch dev:server",
    "dev:watch": "cross-env NODE_ENV=dev rollup -c -w",
    "dev:server": "nodemon --exec omp-server.exe --watch dist/bundle.js --delay 3",
    "build": "cross-env NODE_ENV=prod rollup -c",
    "lint": "eslint src/* --fix -c .eslintrc"
  },
  "keywords": [
    "sa-mp",
    "sa:mp",
    "open.mp",
    "omp",
    "gamemode",
    "samp-node"
  ],
  "author": "YuCarl77",
  "license": "ISC",
  "dependencies": {
    "samp-node-lib": "^1.1.5"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@sa-mp/node": "^0.2.0",
    "@tsconfig/node16": "^1.0.3",
    "@types/node": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.77.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  }
}
